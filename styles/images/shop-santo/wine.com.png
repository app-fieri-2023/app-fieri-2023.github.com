    postRender: function(campaignName, modalContext) {
        // attach events here
        const containerStart = document.querySelector('.emailCaptureModal_container')
        const containerSuccess = document.querySelector('.emailCaptureModal_container.success')
        const captureEmail = document.querySelector('.emailCaptureModal_form');
        captureEmail.addEventListener('submit', async (event) => {
            event.preventDefault();
            if (this.isSubmitting) {
                return;
            }

            this.isSubmitting = true;

            if (document.querySelector('.emailCaptureModal_text').value === '') {
                document.querySelector('.emailCaptureModal_error').innerHTML = 'Missing email address';
                document.querySelector('.emailCaptureModal_error').classList += ' shown';
                this.isSubmitting = false;
                return;
            }
            try {
                await view.captureEmail({
                    brazeEventName: view.shoppingCart.get('itemQuantityInCart') >= 1 ? 'Email_Capture_Abandon_Cart_Promo' : 'Email_Capture_Generic_Exit_Promo',
                    brazeEventData: {
                        CartId: view.storefrontModel.get('shoppingCart').get('id')
                    },
                    email: document.querySelector('.emailCaptureModal_text').value,
                    emailCaptureCampaign: campaignName,
                    list: 'Core',                               // Core or Picked
                    source: view.shoppingCart.get('source')
                });

                // show success modal
                containerStart.classList.add('start-hide');
                containerSuccess.classList.add('success-show');
            } catch (error) {
                // hide spinner
                if (error.responseJSON && error.responseJSON.errors && error.responseJSON.errors.email) {
                    document.querySelector('.emailCaptureModal_error').innerHTML = error.responseJSON.errors.email;
                } else {
                    document.querySelector('.emailCaptureModal_error').innerHTML = 'Unknown error';
                }
                document.querySelector('.emailCaptureModal_error').classList += ' shown';
                this.isSubmitting = false;
            }
        });
    },
    onClose: function (campaignName, modalContext) {
        // clean up events here
    }
